  "devDependencies": {
    "@types/node": "^18.7.14",
    "eslint": "^7.28.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "npm-run-all": "4.1.5",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  },
  "engines": {
    "node": ">=16.0.0",
